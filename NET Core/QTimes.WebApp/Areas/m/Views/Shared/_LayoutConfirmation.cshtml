<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1,user-scalable=0" />
    <title>
        QTimes - Confirmation
    </title>
    <link href="https://fonts.googleapis.com/css?family=Nunito+Sans:300,400,600,700,800,900&display=swap" rel="stylesheet">
    <environment include="Development">
        <link href="~/Content/assets/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
        <link href="~/Content/mReservation/style.css" rel="stylesheet" />
        <link href="~/Content/mReservation/custom.css" rel="stylesheet" />
    </environment>
    <environment exclude="Development">
        <link href="/css/confirmation.min.css" rel="stylesheet" />
    </environment>

    @Html.Raw(JavaScriptSnippet.FullScript)

    <environment include="Development">
        <script src="~/js/jquery/dist/jquery.min.js"></script>
        <script src="~/Content/assets/bootstrap/js/bootstrap.min.js"></script>
        <script src="~/Scripts/moment.min.js"></script>
        <script src="~/Scripts/bwip-js-min.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="/js/confirmation.min.js"></script>
    </environment>
</head>
<body class="booking_confirmation_body">
    <div class="loader hidden">
        <div class="loader_txt_wrap">
            <div class="loader_txt"></div>
            <span class="lode">Processing....</span>
        </div>
    </div>
    <div class="app">
        <div class="container">
            <div class="app__container">
                <div class="waittime">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>

</body>
</html>