    function ContactSupport() {
        event.preventDefault();
        var response = grecaptcha.getResponse();

        if (response.length === 0) {
            $.notify("Please check the reCAPTCHA");
        } else {
            $.ajax({
                type: "POST",
                url: "@Url.Action("ContactSupport", "Home")",
                data: { Name: $("#txtname").val(), Email: $("#txtemail").val(), BusinessName: $("#txtbusinessname").val(), Phone: $("#txtphone").val(), Message: $("#txtmessage").val() },
                success: function (data) {
                    if (data.status == "ok") {
                        NotifySavedMessage("Thanks for contacting QTimes.");
                    }
                    else if (data.status == "error") {
                        NotifyErrorMessage(data.ErrorMessage);
                    }
                },
                error: function (xhr, status, error) {
                    ShowErrorMessage(xhr, status, error);
